CREATE TABLE IF NOT EXISTS Player(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    surname TEXT NOT NULL,
    Games INTEGER,
    FOREIGN KEY(Games) REFERENCES Games(id)
);

selectAll:
SELECT * FROM Player;

selectById:
SELECT * FROM Player WHERE id = ?;

insert:
INSERT INTO Player(name, surname, Games) VALUES (?, ?, ?);

deleteAll:
DELETE FROM Player;
