<Window x:Class="Pt5._1.DeleteRecordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pt5._1"
        mc:Ignorable="d"
        Title="DeleteRecordWindow" Height="452" Width="453">
    <Grid Margin="0,0,0,-6">
        <Label x:Name="IdText" Content="Id&#xD;&#xA;" HorizontalAlignment="Left" Margin="171,32,0,0" VerticalAlignment="Top" Height="26"/>
        <TextBox x:Name="textId" HorizontalAlignment="Center" Margin="0,36,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="60" TextChanged="textId_TextChanged"/>
        <Button x:Name="buttonBuscar" Content="Buscar" HorizontalAlignment="Left" Margin="178,76,0,0" VerticalAlignment="Top" Height="21" Width="78" Click="buttonBuscar_Click"/>
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="78,126,0,0" VerticalAlignment="Top"/>
        <Label Content="Apellido&#xA;" HorizontalAlignment="Left" Margin="78,195,0,0" VerticalAlignment="Top"/>
        <Label Content="Direccion&#xA;" HorizontalAlignment="Left" Margin="78,261,0,0" VerticalAlignment="Top"/>
        <Label Content="Telefono" HorizontalAlignment="Left" Margin="78,325,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="NombreTextBox" MaxLength="25"  HorizontalAlignment="Left" Margin="157,130,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="119" IsEnabled="False"/>
        <TextBox x:Name="ApellidoTextBox" MaxLength="35"  HorizontalAlignment="Left" Margin="157,198,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"  IsEnabled="False"/>
        <TextBox x:Name="DireccionTextBox" MaxLength="50" HorizontalAlignment="Left" Margin="157,264,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"  IsEnabled="False"/>
        <TextBox x:Name="TelefonoTextBox" MaxLength="11"  HorizontalAlignment="Left" Margin="157,329,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"  IsEnabled="False"/>
        <Button x:Name="buttonDelete" Content="Eliminar" HorizontalAlignment="Center" Margin="0,390,0,0" VerticalAlignment="Top"  Height="21" Width="78" Click="buttonDelete_Click"/>

    </Grid>
</Window>
